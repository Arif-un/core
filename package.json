{
  "name": "eruption",
  "private": false,
  "version": "2.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "commit"
  },
  "dependencies": {
    "react": "^18.x",
    "react-dom": "^18.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.x",
    "@commitlint/config-conventional": "^17.x",
    "@commitlint/prompt-cli": "^17.0.3",
    "@fvilers/disable-react-devtools": "^1.x",
    "@testing-library/jest-dom": "^5.x",
    "@testing-library/react": "^13.x",
    "@testing-library/user-event": "^14.x",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "@vitejs/plugin-react": "^1.x",
    "@vitest/ui": "^0.x",
    "c8": "^7.x",
    "eslint": "^8.x",
    "eslint-config-prettier": "^8.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jsx-a11y": "^6.x",
    "eslint-plugin-prettier": "^4.x",
    "eslint-plugin-react": "^7.x",
    "eslint-plugin-react-hooks": "^4.x",
    "happy-dom": "^5.x",
    "husky": "^8.x",
    "lint-staged": "^13.x",
    "prettier": "^2.x",
    "typescript": "^4.x",
    "vite": "^3.x",
    "vite-tsconfig-paths": "^3.x",
    "vitest": "^0.x"
  },
  "keywords": [
    "react",
    "boilerplate",
    "vite",
    "bundle",
    "typescript"
  ]
}
